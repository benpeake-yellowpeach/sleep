{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "spacing": {
      "blockGap": false,
      "margin": false,
      "padding": [
        "top",
        "bottom"
      ],
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "Small",
          "size": "40px",
          "slug": "small"
        },
        {
          "name": "Regular",
          "size": "80px",
          "slug": "regular"
        },
        {
          "name": "Large",
          "size": "160px",
          "slug": "large"
        }
      ]
    },
    "color": {
      "background": true,
      "text": true,
      "link": false,
      "defaultPalette": false,
      "palette": [
        {
          "color": "#171C45",
          "name": "Blue Primary",
          "slug": "dark-bg-background-primary-bg-fill"
        },
        {
          "color": "#1F2453",
          "name": "Blue Secondary",
          "slug": "dark-bg-background-secondary-bg-fill"
        },
        {
          "color": "#FEF7F2",
          "name": "Cream Primary",
          "slug": "light-bg-background-primary-bg-fill"
        },
        {
          "color": "#FDEFE4",
          "name": "Cream Secondary",
          "slug": "light-bg-background-secondary-bg-fill"
        }
      ]
    }
  }
}